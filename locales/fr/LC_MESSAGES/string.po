msgid "LED Color"
msgstr "Couleur de la LED"

#: ../fido2/shared/index-content1.rst:76
msgid "Event"
msgstr "Événement"

#: ../fido2/shared/index-content1.rst:76
msgid "Time Period"
msgstr "Période de temps"

