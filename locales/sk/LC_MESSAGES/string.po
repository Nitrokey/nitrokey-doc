msgstr "Obnovenie hesla používateľa"

#: ../nitropad/qubes/user-password-reset.rst:4
msgid "Follow the next five steps to boot from the installation media"
msgstr ""
"Ak chcete spustiť počítač z inštalačného média, postupujte podľa "
"nasledujúcich piatich krokov"

#: ../nitropad/qubes/user-password-reset.rst:-1
msgid "Step 1"
msgstr "Krok 1"

#: ../nitropad/qubes/user-password-reset.rst:-1
msgid "Step 2"
msgstr "Krok 2"

#: ../nitropad/qubes/user-password-reset.rst:-1
msgid "Step 3"
msgstr "Krok 3"

#: ../nitropad/qubes/user-password-reset.rst:-1
msgid "Step 4"
msgstr "Krok 4"

#: ../nitropad/qubes/user-password-reset.rst:-1
